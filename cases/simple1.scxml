<?xml version="1.0" ?>
<scxml initial="a" version="1.0">
    <name>Simple statechart</name>

    <state id="a">
        <transition event="alpha" target="b"/>
    </state>

    <state id="b">
        <transition event="beta" target="c"/>
    </state>

    <state id="c">
        <final/>
    </state>

</scxml>
